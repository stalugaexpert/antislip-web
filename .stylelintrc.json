{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "unit-allowed-list": ["rem", "vh", "vw", "s", "deg", "%"],
    "order/order": [
      "custom-properties",
      "declarations"
    ],
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  }
}