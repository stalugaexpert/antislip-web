{
  "extends": "stylelint-config-standard-scss",
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "unit-allowed-list": ["rem", "vh", "vw", "s", "deg", "%"],
    "selector-class-pattern": null,
    "order/order": [
      "custom-properties",
      "declarations"
    ],
    "at-rule-no-unknown": null,
    "scss/dollar-variable-empty-line-before": null,
    "scss/at-rule-no-unknown": null
  }
}